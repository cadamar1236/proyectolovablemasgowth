{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "webapp",
  "compatibility_date": "2025-10-21",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "webapp-production",
      "database_id": "local-only"
    }
  ],
  "ai": {
    "binding": "AI"
  },
  "vars": {
    "GROQ_API_KEY": "your_groq_api_key_here"
  }
}